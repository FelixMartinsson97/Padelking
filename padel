    <script>

        (function ($) {

            const select = $('.wpcr-shortcode-popup__select');
            const dropdown = $(' .wpcr-shortcode-popup__dropdown');
            const links = $('.wpcr-shortcode-popup__link');
            const options = $('.wpcr-shortcode-popup__option');
            const button = $('.wpcr-shortcode-popup__button');


            function __no_default_action(e) {
                e.stopPropagation();
                e.preventDefault();
                return false;
            }

            function __toggle_dropdown(e) {

                select.toggleClass('active');

                if (select.hasClass('active')) {
                    dropdown.slideDown(200);
                } else {
                    dropdown.slideUp(200);
                }

                if (e) {
                    __no_default_action(e);
                }
            }

            function __close_dropdown() {
                select.removeClass('active');
                dropdown.slideUp(200);
            }

            function __change_selected_text(elem) {
                select.text($(elem).text());
            }

            function __change_destination(e) {

                options.removeClass('selected');
                $(e.currentTarget).parents('.wpcr-shortcode-popup__option').addClass('selected');
                __change_selected_text(e.currentTarget);
                __close_dropdown();

                if (e) {
                    __no_default_action(e);
                }
            }

            function __set_cookie(name, value, days) {
                let expires = '';
                if (days) {
                    let date = new Date();
                    date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
                    expires = "; expires=" + date.toGMTString();
                }
                document.cookie = name + "=" + value + expires + "; path=/";
            }


            __change_selected_text('.wpcr-shortcode-popup__option.selected .wpcr-shortcode-popup__link');
            select.off().on('click', __toggle_dropdown);
            links.off().on('click', __change_destination);

            button.on('click', function () {
                const selected_elem = $('.wpcr-shortcode-popup__option.selected .wpcr-shortcode-popup__link');
                __set_cookie('__wpcr_chosen_destination', selected_elem.data('location'), 4000);
                window.location = selected_elem.attr('href');
            });

        })(jQuery)


    </script>
	</div>
	</div>
</div>
